{
    "manifest_version": 3,
    "name": "Enhanced New Tab",
    "version": "1.2",
    "description": "A beautiful new tab page with favorites, top sites and recent sites optimized for low-end devices",
    "permissions": [
        "history",
        "storage",
        "favicon",
        "topSites"
    ],
    "optional_permissions": [
        "activeTab"
    ],
    "chrome_url_overrides": {
        "newtab": "new-tab-page.html"
    }
}
